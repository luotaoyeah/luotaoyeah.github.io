<% if (site.tags.length){ %>
<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">TAGS</h3>
    </div>
    <div class="panel-body">
        <%- list_tags({
            show_count: theme.show_count,
            class: 'label label-default sidebar-module',
            style: 'span',
            separator: '&nbsp;'
        }) %>
    </div>
</div>
<% } %>
