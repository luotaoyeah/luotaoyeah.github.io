<% if (site.posts.length){ %>
<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">ARCHIVES</h3>
    </div>
    <div class="panel-body">
        <%- list_archives({
            show_count: theme.show_count,
            type: theme.archive_type,
            class: 'sidebar-module'
        }) %>
    </div>
</div>
<% } %>
