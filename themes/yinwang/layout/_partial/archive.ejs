<%- partial('_partial/nav') %>
<div class="row"
     style="padding: 100px 0 0 0;margin: 0;">
    <div class="container center-block">
        <div class="col-md-12 col-sm-12">
            <div class="panel panel-default">
                <ul class="list-group">
                    <% if (pagination == 2){ %>
                    <% page.posts.each(function(post){ %>
                    <%- partial('article', {
                        post: post,
                        index: true
                    }) %>
                    <% }) %>
                    <% } %>
                </ul>
            </div>
        </div>
    </div>
</div>
</div>
