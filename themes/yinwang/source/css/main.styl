// Components
@import "_common/components";

// font size
$font-size-base = 14px
$font-size-small = $font-size-base - 2px
$font-size-smaller = $font-size-base - 4px
$font-size-large = $font-size-base + 2px
$font-size-larger = $font-size-base + 4px

body {
  margin: 100px auto;
  width: 70%;
  border-radius: 3px;
}